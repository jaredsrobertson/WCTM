def get_rank(steam_id):
    driver.get(f"https://rocketleague.tracker.network/rocket-league/profile/steam/{steam_id}/overview")
    ranks_table = wait.until(ec.visibility_of_element_located((By.CSS_SELECTOR, "div.trn-table__container>table>tbody")))
    rank_3v3 = ranks_table.find_element(By.XPATH, "//tr//td[@class='name']//div[contains(text(),'Standard 3v3')]/following-sibling::div")

    rank = rank_3v3.text #full rank
    #driver.close()
    num = len(rank.split()[1])
    rankimg = rank[:1].lower() + str(num)
    return rank, rankimg